<h2 class="page-header">Register</h2>
{{#if user}}
        <div class="alert alert-danger">Data has taken already try new</div> 
{{/if}}
{{#if mail}}
        <div class="alert alert-danger">nodeNumber has taken already try new</div> 
{{/if}} 
{{#if errors}}
      {{#each errors}}
        <div class="alert alert-danger">{{msg}}</div>
      {{/each}}
{{/if}}
<form method="post" action="/users/register">
   <div class="form-group">
    <label>Date</label>
    <input type="date" class="form-control" placeholder="date" name="Date">
  </div>
  <div class="form-group">
    <label>data</label>
    <input type="text" class="form-control" placeholder="owner id of node" name="data">
  </div>
   <div class="form-group">
    <label>nodeNumber</label>
    <input type="number" class="form-control" placeholder="Integer value" name="nodeNumber">
  </div>
  <div class="form-group">
    <label>nodeId</label>
    <input type="text" class="form-control" placeholder="node" name="nodeId">
  </div>
  <div class="form-group">
    <label>referenceNodeId</label>
    <input type="text" class="form-control" placeholder="node" name="childNodeId">
  </div>
  <div class="form-group">
    <label>childReferenceId</label>
    <input type="text" class="form-control" placeholder="node" name="chidrefId">
  </div>
  <div class="form-group">
    <label>genesisReferenceId</label>
    <input type="text" class="form-control" placeholder="node" name="genesisRefId">
  </div>
  <div class="form-group">
    <label>Password</label>
    <input type="password" class="form-control" placeholder="Password" name="password">
  </div>
  <button type="submit" class="btn btn-info btn-block">Submit</button>
</form>